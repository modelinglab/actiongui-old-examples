<ns1:window-layout ns1:id="patientsWi"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
        <ns1:entry>
            <ns1:widget ns1:id="medicalCenterLa" ns1:width="300px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="40px"></ns1:line-layout>
        </ns1:entry>
        
        <ns1:entry>
            <ns1:widget ns1:id="patientsLa" ns1:width="300px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:table ns1:id="patientsTa" ns1:width="700px" ns1:height="300px">
                <ns1:column>
                    <ns1:widget ns1:id="name" ns1:height="30px"></ns1:widget>
                </ns1:column>
                <ns1:column>
                    <ns1:widget ns1:id="surname" ns1:height="30px"></ns1:widget>
                </ns1:column>
                <ns1:column>
                    <ns1:widget ns1:id="gender" ns1:height="30px"></ns1:widget>
                </ns1:column>
                <ns1:column>
                    <ns1:widget ns1:id="floor" ns1:height="30px"></ns1:widget>
                </ns1:column>
                <ns1:column>
                    <ns1:widget ns1:id="room" ns1:height="30px"></ns1:widget>
                </ns1:column>
                <ns1:column>
                    <ns1:widget ns1:id="socialInfo" ns1:height="30px"></ns1:widget>
                </ns1:column>
            </ns1:table>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="40px"></ns1:line-layout>
        </ns1:entry>
        
        <ns1:entry>
            <ns1:widget ns1:id="movePatientBu" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:widget ns1:id="backBu" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
    </ns1:line-layout>

</ns1:window-layout>
