<ns1:window-layout ns1:id="createProfessionalWi"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
        <!-- General information -->
        <ns1:entry>
            <ns1:widget ns1:id="generalInfoLa" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="nameLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="nameEn" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="surnameLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="surnameEn" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="40px"></ns1:line-layout>
        </ns1:entry>
        
        <!-- Login information -->
        <ns1:entry>
            <ns1:widget ns1:id="loginInfoLa" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="userLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="userEn" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="passwordLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="passwordEn" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="40px"></ns1:line-layout>
        </ns1:entry>
        
        <!-- kind of professional -->
        <ns1:entry>
            <ns1:widget ns1:id="chooseProfLa" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="doctorLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="doctorBf" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="doctorLicenseLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="doctorLicenseEn" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="statusLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="statusBf" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="nurseLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="nurseBf" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="nursingLicenseLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="nursingLicenseEn" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="administrativeLa" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="administrativeBf" ns1:width="125px" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="40px"></ns1:line-layout>
        </ns1:entry>
        
        <!-- Choose a department -->
        <ns1:entry>
            <ns1:widget ns1:id="chooseDepartmentLa" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:table ns1:id="departmentsTa" ns1:width="200px" ns1:height="200px">
                <ns1:column>
                    <ns1:widget ns1:id="departmentLa" ns1:height="30px"></ns1:widget>
                </ns1:column>
            </ns1:table>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="40px"></ns1:line-layout>
        </ns1:entry>
        
        <ns1:entry>
            <ns1:widget ns1:id="createProfessionalBu" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
        <ns1:entry>
            <ns1:widget ns1:id="cancelBu" ns1:width="200px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
    </ns1:line-layout>
</ns1:window-layout>
