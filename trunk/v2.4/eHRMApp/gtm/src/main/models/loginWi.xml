                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal"  ns1:height="auto" ns1:width="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="passLa" ns1:height="30px" ns1:width="150px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="passEn" ns1:height="30px" ns1:width="150px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:widget ns1:id="loginBu" ns1:height="30px" ns1:width="150px"></ns1:widget>
        </ns1:entry>
    </ns1:line-layout>

