<ns1:window-layout
    ns1:id="SalesPersonMenu_W"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemas/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemas/ag-layout/2.4 http://actiongui.org/schemas/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="horizontal" ns1:height="100%">
        <ns1:entry expand-ratio="1">
            <ns1:line-layout orientation="horizontal"/>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="100%" ns1:width="200px">
                <ns1:entry expand-ratio="1">
                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="Manager_M_SearchCustomers_W_B" ns1:height="30px" ns1:width="100%"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/> 
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="Manager_M_ChangePassword_W_B" ns1:height="30px" ns1:width="100%"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/> 
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="Manager_M_Login_W_B" ns1:height="30px" ns1:width="100%"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/> 
                </ns1:entry>
                <ns1:entry expand-ratio="1">
                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry expand-ratio="1">
            <ns1:line-layout orientation="horizontal"/>
        </ns1:entry>
    </ns1:line-layout>

</ns1:window-layout>
