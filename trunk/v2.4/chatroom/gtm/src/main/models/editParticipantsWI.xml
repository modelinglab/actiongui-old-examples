<ns1:window-layout ns1:id="editParticipantsWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                <ns1:entry>
                    <ns1:table ns1:id="usersTA" ns1:width="250px" ns1:height="250px">
                        <ns1:column>
                            <ns1:widget ns1:id="users" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="addBU" ns1:width="70px" ns1:height="70px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="110px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="removeBU" ns1:width="70px" ns1:height="70px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="participantsTA" ns1:width="250px" ns1:height="250px">
                        <ns1:column>
                            <ns1:widget ns1:id="participants" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:widget ns1:id="backBU" ns1:width="100px" ns1:height="30px"></ns1:widget>
        </ns1:entry>
    </ns1:line-layout>
</ns1:window-layout>
