<?xml version="1.0" encoding="UTF-8"?>

<ns1:window-layout ns1:id="UserAccountsWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" left-margin="20px" ns1:height="auto">
    <!--FIRST TABLE -->
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="UsersTB_page" ns1:height="30px" ns1:width="600px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="UsersTB" ns1:height="200px" ns1:width="600px">
                        <ns1:column>
                            <ns1:widget ns1:id="surname" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="name" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="login" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                         <ns1:column>
                            <ns1:widget ns1:id="myRole" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="UsersTB_prev_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="30px" ns1:width="30px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="UsersTB_next_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="30px"></ns1:line-layout>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
         <ns1:entry>
            <ns1:widget ns1:id="CreateBU" ns1:height="30px" ns1:width="60px"></ns1:widget>
        </ns1:entry>
  </ns1:line-layout> 
</ns1:window-layout>
