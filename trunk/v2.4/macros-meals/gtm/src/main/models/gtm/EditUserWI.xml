<?xml version="1.0" encoding="UTF-8"?>

<ns1:window-layout ns1:id="EditUserWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="400px">
        <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_name_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_name" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
                <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_surname_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_surname" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
                <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_login_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_login" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
                <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_password_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_password" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_role_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="ThisUserFO_role" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <!-- Update -->
        <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="thisUserUpdateBU" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry> 
            </ns1:line-layout>
        </ns1:entry>
        <!-- Periods -->
        <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="editUserPeriodsBU" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry> 
            </ns1:line-layout>
        </ns1:entry>
        <!-- BACK -->
        <ns1:entry>
            <ns1:widget ns1:id="BackBU" ns1:height="30px" ns1:width="60px"></ns1:widget>
        </ns1:entry>   
  </ns1:line-layout> 
</ns1:window-layout>
