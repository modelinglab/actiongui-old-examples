<?xml version="1.0" encoding="UTF-8" ?> 
<!-- was: no XML declaration present -->
<ns1:window-layout ns1:id="LoginWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
 <ns1:line-layout orientation="horizontal" ns1:height="100%">
     <ns1:entry>
         <ns1:line-layout orientation="horizontal" ns1:width="30px"/>
     </ns1:entry>
     <!-- CENTER -->
    <ns1:entry>
        <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="1200px">   
            <!-- TOP -->
            <ns1:entry>
                <ns1:line-layout orientation="vertical" ns1:height="30px"/>
            </ns1:entry> 
        <!-- MIDDLE -->
                    <ns1:entry>
                        <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                            <!-- MIDDLE-LEFT -->
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:width="100px">
                                    <ns1:entry>
                                        <ns1:widget ns1:id="logoIMG"></ns1:widget>
                                    </ns1:entry>
                                </ns1:line-layout>
                            </ns1:entry>
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:width="30px" />
                            </ns1:entry>
                            <!-- MIDDLE CENTER -->
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
                                    <ns1:entry>
                                        <ns1:line-layout orientation="vertical" ns1:height="15px"/>
                                    </ns1:entry>
                                    <ns1:entry>
                                        <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                                            <ns1:entry>
                                                <ns1:widget ns1:id="LoginFO_login_L" ns1:height="30px" ns1:width="80px"></ns1:widget>
                                            </ns1:entry>
                                            <ns1:entry>
                                                <ns1:widget ns1:id="LoginFO_login" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                            </ns1:entry>
                                        </ns1:line-layout>
                                    </ns1:entry>
                                    <ns1:entry>
                                        <ns1:line-layout orientation="vertical" ns1:height="15px"/>
                                    </ns1:entry> 
                                    <ns1:entry>
                                        <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                                            <ns1:entry>
                                                <ns1:widget ns1:id="LoginFO_password_L" ns1:height="30px" ns1:width="80px"></ns1:widget>
                                            </ns1:entry>
                                            <ns1:entry>
                                                <ns1:widget ns1:id="LoginFO_password" ns1:height="30px" ns1:width="120px"></ns1:widget>    
                                            </ns1:entry>
                                        </ns1:line-layout>
                                    </ns1:entry>
                                    <ns1:entry>
                                        <ns1:line-layout orientation="vertical" ns1:height="15px"/>
                                    </ns1:entry> 
 
                                    <ns1:entry>
                                        <ns1:widget ns1:id="LoginBU" ns1:height="30px" ns1:width="100px"></ns1:widget>
                                    </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <!-- END MIDDLE-CENTER -->
                    </ns1:line-layout>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <!-- END CENTER -->
    <ns1:entry expand-ratio="1">
        <ns1:line-layout orientation="horizontal"/>
    </ns1:entry>
</ns1:line-layout>
</ns1:window-layout>