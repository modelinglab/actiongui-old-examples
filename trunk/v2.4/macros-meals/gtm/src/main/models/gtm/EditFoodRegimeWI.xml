<?xml version="1.0" encoding="UTF-8"?>

<ns1:window-layout ns1:id="EditFoodRegimeWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="400px">
        <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="FoodRegimeFO_name_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="FoodRegimeFO_name" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
                <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="FoodRegimeFO_description_L" ns1:height="30px" ns1:width="100px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="FoodRegimeFO_description" ns1:height="200px" ns1:width="300px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <!-- UPDATE -->
        <ns1:entry>
            <ns1:line-layout orientation="horizontal">
                <ns1:entry>
                    <ns1:widget ns1:id="UpdateBU" ns1:height="30px" ns1:width="120px"></ns1:widget>
                </ns1:entry> 
            </ns1:line-layout>
        </ns1:entry>
        <!-- BACK -->
        <ns1:entry>
            <ns1:widget ns1:id="BackBU" ns1:height="30px" ns1:width="60px"></ns1:widget>
        </ns1:entry>   
  </ns1:line-layout> 
</ns1:window-layout>



