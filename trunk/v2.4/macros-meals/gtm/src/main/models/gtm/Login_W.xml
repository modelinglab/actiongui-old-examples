<ns1:window-layout ns1:id="Login_w"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" left-margin="20px" ns1:height="auto">
        <!--FIRST TABLE -->
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="Meals_T_page" ns1:height="30px" ns1:width="100px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="Meals_T" ns1:height="200px" ns1:width="300px">
                        <ns1:column>
                            <ns1:widget ns1:id="literal" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meals_T_prev_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="30px" ns1:width="30px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Meals_T_next_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="30px"></ns1:line-layout>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
      
        <!-- SECOND TABLE -->
        <ns1:entry>          
            <ns1:line-layout orientation="vertical" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="Meal_Options_T_page" ns1:height="30px" ns1:width="100px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="Meal_Options_T" ns1:height="200px" ns1:width="300px">
                        <ns1:column>
                            <ns1:widget ns1:id="literal" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Options_T_prev_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="30px" ns1:width="30px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Options_T_next_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="50px"></ns1:line-layout>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>  
        
        <!-- THIRD TABLE -->
        <ns1:entry>           
            <ns1:line-layout orientation="vertical" bottom-margin="100px" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="Meal_Option_Deadlines_T_page" ns1:height="30px" ns1:width="100px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="Meal_Option_Deadlines_T" ns1:height="200px" ns1:width="300px">
                        <ns1:column>
                            <ns1:widget ns1:id="cday" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="chour" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="cminute" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Option_Deadlines_T_prev_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="30px" ns1:width="30px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Option_Deadlines_T_next_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="50px"></ns1:line-layout>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
    </ns1:line-layout>

</ns1:window-layout>
