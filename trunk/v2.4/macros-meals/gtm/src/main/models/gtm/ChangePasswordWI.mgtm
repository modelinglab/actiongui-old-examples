Window ChangePasswordWI {

#form ChangePasswordFO {
    entity : User
    instance : [$ChangePasswordWI.caller$]
    prop: password {label: ["New password: "] }
}#

Button ChangePasswordBU {
    String text := ['Change']
    event onClick {
    #update {
        form: ChangePasswordFO
        assert: [true] {message: ['Your password has been changed']}
    }#
}
}
}
