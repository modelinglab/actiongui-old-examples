                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"></ns1:line-layout>
                </ns1:entry>
                <!-- New Option -->
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="NewPeriodOptionFO_start_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="NewPeriodOptionFO_start" ns1:height="30px" ns1:width="120px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>    
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="NewPeriodOptionFO_end_L" ns1:height="30px" ns1:width="60px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="NewPeriodOptionFO_end" ns1:height="30px" ns1:width="120px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>      
                </ns1:entry>
                <!-- END CALENDARS -->
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
