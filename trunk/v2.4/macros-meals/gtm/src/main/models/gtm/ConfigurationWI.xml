                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="DaysBefore" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                         <ns1:column>
                            <ns1:widget ns1:id="UpDB" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="DownDB" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="HourLimit" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                         <ns1:column>
                            <ns1:widget ns1:id="UpHL" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="DownHL" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="MinutesLimit" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                         <ns1:column>
                            <ns1:widget ns1:id="UpML" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="DownML" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
