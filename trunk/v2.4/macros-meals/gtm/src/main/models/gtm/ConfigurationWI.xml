                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Options_T_prev_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="30px" ns1:width="30px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Options_T_next_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"></ns1:line-layout>
                </ns1:entry> 
                <!-- New meal option -->
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                                 <ns1:entry>
                                    <ns1:widget ns1:id="NewOptionFO_initial_L" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="NewOptionFO_initial" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                                 <ns1:entry>
                                    <ns1:widget ns1:id="NewOptionFO_literal_L" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="NewOptionFO_literal" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="NewOptionBU" ns1:height="30px" ns1:width="150px"></ns1:widget>
                        </ns1:entry>  
                    </ns1:line-layout>
                </ns1:entry>
                <!-- END New Option -->  
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="30px"></ns1:line-layout>
                </ns1:entry>
                <!-- Edit meal option -->
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                                 <ns1:entry>
                                    <ns1:widget ns1:id="EditOptionFO_initial_L" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="EditOptionFO_initial" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                                 <ns1:entry>
                                    <ns1:widget ns1:id="EditOptionFO_literal_L" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="EditOptionFO_literal" ns1:height="30px" ns1:width="120px"></ns1:widget>
                                </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="EditOptionBU" ns1:height="30px" ns1:width="150px"></ns1:widget>
                        </ns1:entry>  
                    </ns1:line-layout>
                </ns1:entry>
                <!-- END Edit meal Option -->
            </ns1:line-layout>
        </ns1:entry>  
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:height="60px"></ns1:line-layout>
        </ns1:entry>
        
        <!-- THIRD TABLE: Meal option period -->
        <ns1:entry>          
            <ns1:line-layout orientation="vertical" ns1:height="auto">
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="ThisOptionPeriodsFO_page" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="mealOptionPeriodsLA" ns1:width="150px" ns1:height="30px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="ThisOptionPeriodsFO" ns1:height="100px" ns1:width="auto">
                        <ns1:column>
                            <ns1:widget ns1:id="start" ns1:height="30px" ns1:width="150px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="end" ns1:height="30px" ns1:width="150px"></ns1:widget>
                        </ns1:column>           
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
