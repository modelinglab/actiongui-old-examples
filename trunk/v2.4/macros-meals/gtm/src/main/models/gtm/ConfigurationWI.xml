                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="EditMealBU" ns1:height="30px" ns1:width="100px"></ns1:widget>
                </ns1:entry> 
                <!-- END Edit Meal --> 
           </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:height="60px"></ns1:line-layout>
        </ns1:entry> 
        <!-- SECOND TABLE: Meal Option-->
        <ns1:entry>          
            <ns1:line-layout orientation="vertical" ns1:height="auto">
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meal_Options_T_page" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="mealOptionsLA" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="Meal_Options_T" ns1:height="200px" ns1:width="auto">
                        <ns1:column>
                            <ns1:widget ns1:id="initial" ns1:height="30px" ns1:width="50px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
