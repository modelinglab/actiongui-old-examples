                        </ns1:entry> 
                    </ns1:line-layout>
                </ns1:entry>
                <!-- END New Period -->  
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:height="60px"></ns1:line-layout>
        </ns1:entry>
        <!-- FOURTH TABLE: deadline -->

        <ns1:entry>           
            <ns1:line-layout orientation="vertical" bottom-margin="100px" ns1:height="auto">

                <ns1:entry>
                    <ns1:table ns1:id="DeadlinesTB" ns1:height="200px" ns1:width="600px">
                        <ns1:column>
                            <ns1:widget ns1:id="Monday" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="Tuesday" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="Wednesday" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="Thursday" ns1:height="30px" ns1:width="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
