<ns1:window-layout ns1:id="ConfigurationWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" left-margin="30px" ns1:height="auto">
        <!--FIRST TABLE: Meal -->        
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meals_T_page" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="mealsLA" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="Meals_T" ns1:height="200px" ns1:width="auto">
                        <ns1:column>
                            <ns1:widget ns1:id="literal" ns1:height="30px" ns1:width="300px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <!-- table next and prev -->
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="Meals_T_prev_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="30px" ns1:width="30px"></ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Meals_T_next_B" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"></ns1:line-layout>
                </ns1:entry> 
                <!-- New Meal -->
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="NewMealFO_literal_L" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="NewMealFO_literal" ns1:height="30px" ns1:width="120px"></ns1:widget>
                        </ns1:entry>  
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="NewMealBU" ns1:height="30px" ns1:width="100px"></ns1:widget>
                </ns1:entry>  
                <!-- END New Meal -->  
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="30px"></ns1:line-layout>
                </ns1:entry>
                <!-- Edit Meal -->
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                        <ns1:entry>
                            <ns1:widget ns1:id="EditMealFO_literal_L" ns1:height="30px" ns1:width="100px"></ns1:widget>
                        </ns1:entry>
                         <ns1:entry>
                            <ns1:widget ns1:id="EditMealFO_literal" ns1:height="30px" ns1:width="120px"></ns1:widget>
