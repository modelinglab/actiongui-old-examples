entity Employee isUser {
    String login
    String password 
    Role myRole
    Set(Customer) customers oppositeTo contacts
}

entity Customer {
    String name
    String surname
    Date birthday
    String email
    String phone
    Integer loyalty

    Set(Employee) contacts oppositeTo customers
}

enum Role {
    MANAGER // 0
    SALESMANAGER // 1
    DEFAULT // 2
}