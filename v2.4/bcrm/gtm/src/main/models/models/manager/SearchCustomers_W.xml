<ns1:window-layout
    ns1:id="SearchCustomers_W"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="horizontal" ns1:height="100%">
        <ns1:entry expand-ratio="1">
            <ns1:line-layout orientation="horizontal"/>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="100%" ns1:width="auto">
                <ns1:entry expand-ratio="1">
                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:table ns1:id="Customers_T" ns1:width="600px" ns1:height="400px">
                        <ns1:column>
                            <ns1:widget ns1:id="name" ns1:width="70px"/>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="surname" ns1:width="70px"/>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="birthday" ns1:width="100px"/>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="email" ns1:width="100%"/>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="phone" ns1:width="60px"/>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="loyalty" ns1:width="60px"/>
                        </ns1:column>
                    </ns1:table>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="25px">
                        <ns1:entry>
                            <ns1:widget ns1:id="Customers_T_prev_B" ns1:width="70px"/>
                        </ns1:entry>
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Customers_T_page" ns1:width="30px"/>
                        </ns1:entry>
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:widget ns1:id="Customers_T_next_B" ns1:width="70px"/>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="25px"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="horizontal" ns1:height="auto">
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:width="200px">
                                <ns1:entry>
                                    <ns1:widget ns1:id="Edit_B" ns1:height="25px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Delete_B" ns1:height="25px"/>
                                </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry expand-ratio="1">
                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="Back_B" ns1:height="auto"/>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry expand-ratio="1">
            <ns1:line-layout orientation="horizontal"/>
        </ns1:entry>
    </ns1:line-layout>

</ns1:window-layout>
