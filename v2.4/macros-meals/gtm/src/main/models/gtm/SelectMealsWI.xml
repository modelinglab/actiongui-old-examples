<?xml version="1.0" encoding="UTF-8" ?> 
<!-- was: no XML declaration present -->
<ns1:window-layout ns1:id="SelectMealsWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://www.actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
 <ns1:line-layout orientation="horizontal" ns1:height="100%">
     <!-- LEFT PANNEL -->
     <ns1:entry>
         <ns1:line-layout orientation="horizontal" ns1:width="30px" ns1:height="auto"/>
     </ns1:entry>
     <!-- CENTRAL PANNEL -->
    <ns1:entry>
        <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="1200px">   
            <!-- TOP (CENTER PANNEL) -->
            <ns1:entry>
                <ns1:line-layout orientation="vertical" ns1:height="30px"/>
            </ns1:entry> 
            <!-- MIDDLE (CENTER PANNEL) -->
            <ns1:entry>
                <ns1:line-layout orientation="horizontal" ns1:height="auto" ns1:width="auto">
                    <!-- LEFT (MIDDLE CENTER PANNEL)-->
                    <ns1:entry>
                        <ns1:line-layout orientation="vertical" ns1:width="120px">
                            <ns1:entry>
                                <ns1:widget ns1:id="logoIMG"></ns1:widget>
                            </ns1:entry>
                        </ns1:line-layout>
                    </ns1:entry>
                    <!-- END LEFT (MIDDLE CENTER PANNEL) -->
                    <!-- SEPARATOR (MIDDLE CENTER PANNEL) -->
                    <ns1:entry>
                        <ns1:line-layout orientation="vertical" ns1:width="10px" />
                    </ns1:entry>
                    <!-- END SEPARATOR (MIDDLE CENTER PANNEL) -->
                    <!-- CENTER (MIDDLE CENTER PANNEL) -->
                    <ns1:entry>
                        <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
                            <!-- -->    
                            <!-- WINDOW -->
<ns1:entry>
                                <ns1:line-layout orientation="horizontal" ns1:height="auto">
                                    <ns1:entry>
                                        <ns1:widget ns1:id="PreviousWeekBU" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                                    <ns1:entry expand-ratio="1">
                                        <ns1:line-layout orientation="horizontal"/>
                                    </ns1:entry>
                                    <ns1:entry>
                                        <ns1:widget ns1:id="NextWeekBU" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                               </ns1:line-layout>
                            </ns1:entry>
                            <!-- END BUTTONS-->
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:height="15px"/>
                            </ns1:entry> 
                           <!-- LABELS -->
                            <ns1:entry>
                                <ns1:line-layout orientation="horizontal" ns1:height="auto">
                                    <ns1:entry>
                                        <ns1:widget ns1:id="firstDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                                        <ns1:entry>
                                    <ns1:widget ns1:id="secondDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                        </ns1:entry>
                                    <ns1:entry>
                                        <ns1:widget ns1:id="thirdDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                                        <ns1:entry>
                                    <ns1:widget ns1:id="fourthDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                        </ns1:entry>
                                    <ns1:entry>
                                        <ns1:widget ns1:id="fifthDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                                    <ns1:entry>
                                        <ns1:widget ns1:id="sixthDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                                    <ns1:entry>
                                        <ns1:widget ns1:id="seventhDayThisWeekLB" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:entry>
                                </ns1:line-layout>             
                            </ns1:entry>
                            <!-- END LABELS -->
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:height="5px"/>
                            </ns1:entry> 
                            <!-- TABLE -->
                            <ns1:entry>
                                <ns1:table ns1:id="WeekMealsTB" ns1:height="200px" ns1:width="auto">
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Monday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Tuesday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Wednesday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Thursday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Friday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Saturday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                    <ns1:column>
                                        <ns1:widget  ns1:id="Sunday" ns1:height="auto" ns1:width="80px"></ns1:widget>
                                    </ns1:column>
                                </ns1:table>
                            </ns1:entry>
                            <!-- END TABLE -->
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:height="30px"/>
                            </ns1:entry> 
                            <ns1:entry>
                                <ns1:widget ns1:id="changePasswordBU" ns1:width="130px" ns1:height="auto"></ns1:widget>
                            </ns1:entry>
                            <ns1:entry>
                                <ns1:line-layout orientation="vertical" ns1:height="15px"/>
                            </ns1:entry> 
                            <ns1:entry>
                                <ns1:widget ns1:id="logoutBU" ns1:width="130px" ns1:height="auto"></ns1:widget>
                            </ns1:entry>
                            <!-- -->
                        </ns1:line-layout>
                    </ns1:entry>
                    <!-- END CENTER (MIDDLE CENTER PANNEL) -->
                </ns1:line-layout>
            </ns1:entry>
            <!-- END MIDDLE (CENTER PANNEL) -->
        </ns1:line-layout>
    </ns1:entry>
    <!-- END CENTRAL PANNEL -->
    <!-- RIGHT PANNEL -->
    <ns1:entry expand-ratio="1">
        <ns1:line-layout orientation="horizontal"/>
    </ns1:entry>
    <!-- END RIGHT PANNEL -->
</ns1:line-layout>
</ns1:window-layout>


