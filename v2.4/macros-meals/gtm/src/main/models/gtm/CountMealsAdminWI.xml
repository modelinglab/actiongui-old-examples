<ns1:window-layout ns1:id="CountMealsWI"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="auto">
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="DateLA" ns1:height="30px" ns1:width="90px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="DateDF" ns1:height="30px" ns1:width="90px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="horizontal" ns1:width="auto" ns1:height="auto">
                <ns1:entry>
                    <ns1:widget ns1:id="PrevBU" ns1:height="30px"></ns1:widget>
                </ns1:entry>
                <ns1:entry>
                    <ns1:widget ns1:id="NextBU" ns1:height="30px"></ns1:widget>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry>
            <ns1:table ns1:id="MealsTA" ns1:height="600px" ns1:width="auto">
                <ns1:column>
                    <ns1:widget ns1:id="Meals" ns1:width="150px" ns1:height="30px"></ns1:widget>
                </ns1:column>
                <ns1:column>
                    <ns1:table ns1:id="MealOptions" ns1:width="auto" ns1:height="150px">
                        <ns1:column>
                            <ns1:widget ns1:id="Option" ns1:width="250px" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="Count" ns1:width="50px" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="Status" ns1:width="50px" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                        <ns1:column>
                            <ns1:widget ns1:id="Regimes" ns1:width="300px" ns1:height="30px"></ns1:widget>
                        </ns1:column>
                    </ns1:table>
                </ns1:column>
            </ns1:table>
        </ns1:entry>
        <ns1:entry>
            <ns1:widget ns1:id="BackBU" ns1:height="30px" ns1:width="80px"></ns1:widget>
        </ns1:entry>
    </ns1:line-layout>

</ns1:window-layout>
